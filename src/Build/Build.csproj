<Project Sdk="Microsoft.NET.Sdk">

   <PropertyGroup>
      <ManagePackageVersionsCentrally>false</ManagePackageVersionsCentrally>
      <TargetFramework>net10.0</TargetFramework>
      <OutputType>Exe</OutputType>
      <NoWarn>$(NoWarn);CS0169;CS0649;</NoWarn>
      <NukeTelemetryVersion>1</NukeTelemetryVersion>
      <IsPackable>false</IsPackable>
   </PropertyGroup>

   <ItemGroup>
      <PackageDownload Include="GitVersion.Tool" Version="[6.5.0]"/>
      <PackageDownload Include="ReportGenerator" Version="[5.5.0]"/>
   </ItemGroup>

   <ItemGroup>
      <PackageReference Include="Nuke.Common" Version="10.0.0"/>
   </ItemGroup>

   <ItemGroup>
      <Using Include="Nuke.Common"/>
   </ItemGroup>

</Project>
